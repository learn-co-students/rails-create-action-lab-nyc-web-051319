<!-- Create your form here -->
<h2>Student Form</h2>

<%= form_tag students_path do %>
  <label>Student First name:</label><br>
  <input type="text" id="student_first_name" name="first_name"><br>

  <label>Student Last name:</label><br>
  <input type="text" id="student_last_name" name="last_name"><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="submit" value="Submit Student">
<%end%>
